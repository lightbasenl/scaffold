{
  "name": "@lightbasenl/scaffold",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "release": "WITH_SENTRY=true yarn build",
    "pretty": "prettier --check '**/*.{json,md,js,mjs}' --write",
    "format": "yarn pretty && yarn lint --fix",
    "generate": "yarn generate:api",
    "generate:api": "node ./scripts/generate.mjs",
    "generate:svg": "yarn svgr --ignore-existing -d src/assets assets && yarn format",
    "test": "true",
    "test:e2e": "yarn playwright test",
    "test:e2e:ci": "node ./scripts/test-e2e-ci.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.7",
    "@lightbase/next-preset": "2.0.1",
    "@sentry/nextjs": "7.9.0",
    "@tanstack/react-query": "4.0.10",
    "axios": "0.27.2",
    "classnames": "^2.3.1",
    "jwt-decode": "3.1.2",
    "next": "12.2.4",
    "next-i18next": "11.3.0",
    "nookies": "2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-hook-form": "^7.34.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@compas/code-gen": "0.0.211",
    "@compas/stdlib": "0.0.211",
    "@playwright/test": "1.24.2",
    "@svgr/cli": "6.3.1",
    "@tailwindcss/forms": "^0.5.2",
    "@types/node": "18.6.4",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "autoprefixer": "10.4.8",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-no-relative-import-paths": "1.4.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.16",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "14.1.0",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.8",
    "tree-kill": "1.2.2",
    "typescript": "4.7.4"
  },
  "browserslist": [
    "defaults"
  ]
}
