{
  "name": "@lightbasenl/scaffold",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "release": "WITH_SENTRY=true yarn build",
    "pretty": "prettier --check '**/*.{json,md,js,mjs}' --write",
    "format": "yarn pretty && yarn lint --fix",
    "generate": "yarn generate:api",
    "generate:api": "node ./scripts/generate.mjs",
    "generate:svg": "yarn svgr --ignore-existing -d src/assets assets && yarn format",
    "test": "true",
    "test:e2e": "yarn playwright test",
    "test:e2e:ci": "node ./scripts/test-e2e-ci.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "3.1.0",
    "@lightbase/next-preset": "2.0.1",
    "@sentry/nextjs": "7.53.1",
    "@tanstack/react-query": "4.29.12",
    "axios": "1.4.0",
    "classnames": "2.3.2",
    "jwt-decode": "3.1.2",
    "next": "13.4.4",
    "next-i18next": "12.1.0",
    "nookies": "2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-hook-form": "7.44.2",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@compas/cli": "0.10.0",
    "@compas/code-gen": "0.10.0",
    "@compas/stdlib": "0.10.0",
    "@playwright/test": "1.42.1",
    "@svgr/cli": "8.1.0",
    "@tailwindcss/forms": "0.5.7",
    "@types/node": "20.11.30",
    "@types/react": "18.2.72",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.38",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "16.1.0",
    "tailwindcss": "3.4.1",
    "tree-kill": "1.2.2",
    "typescript": "5.4.3"
  },
  "browserslist": [
    "defaults"
  ],
  "workspaces": [
    "vendor/**"
  ]
}
