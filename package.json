{
  "name": "@lightbasenl/scaffold",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "release": "WITH_SENTRY=true yarn build",
    "pretty": "prettier --check '**/*.{json,md,js,mjs}' --write",
    "format": "yarn pretty && yarn lint --fix",
    "generate": "yarn generate:api",
    "generate:api": "node ./scripts/generate.mjs",
    "generate:svg": "yarn svgr --ignore-existing -d src/assets assets && yarn format",
    "test": "true",
    "test:e2e": "yarn playwright test",
    "test:e2e:ci": "node ./scripts/test-e2e-ci.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.2",
    "@lightbase/next-preset": "2.0.1",
    "@sentry/nextjs": "7.86.0",
    "@tanstack/react-query": "5.13.4",
    "axios": "1.6.2",
    "classnames": "2.3.2",
    "jwt-decode": "4.0.0",
    "next": "14.0.4",
    "next-i18next": "15.1.1",
    "nookies": "2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-hook-form": "7.49.0",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@compas/cli": "0.9.0",
    "@compas/code-gen": "0.9.0",
    "@compas/stdlib": "0.9.0",
    "@playwright/test": "1.40.1",
    "@svgr/cli": "8.1.0",
    "@tailwindcss/forms": "0.5.7",
    "@types/node": "20.10.4",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.32",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "15.1.0",
    "tailwindcss": "3.3.6",
    "tree-kill": "1.2.2",
    "typescript": "5.3.3"
  },
  "browserslist": [
    "defaults"
  ],
  "workspaces": [
    "vendor/**"
  ]
}
